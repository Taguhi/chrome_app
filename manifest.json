{
  "manifest_version": 2,

  "name": "Teamable Chrome App",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }, 
  "content_scripts": [
  {
    "matches": ["http://*/*","https://*/*"],
    "js": ["jquery.min.js", "rightbar.js"]
  }
  ],
   "web_accessible_resources": ["rightbar.html"]
}
